<Window x:Class="SortedListApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SortedListApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnAdd" Content="Add" HorizontalAlignment="Left" Height="92" Margin="464,125,0,0" VerticalAlignment="Top" Width="100" Click="btnAdd_Click"/>
        <TextBox x:Name="keytxt" HorizontalAlignment="Left" Margin="308,125,0,0" TextWrapping="Wrap" Text="key" VerticalAlignment="Top" Width="151" Height="36"/>
        <TextBox x:Name="valuetxt" HorizontalAlignment="Left" Margin="308,181,0,0" TextWrapping="Wrap" Text="value" VerticalAlignment="Top" Width="151" Height="36"/>

    </Grid>
</Window>
